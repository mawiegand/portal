}

#switchbar:active .arrow-down {
  background: url('<%= asset_path 'lp/switch_down_btn_pressed.png' %>');
}

.arrow-up {
  background: url('<%= asset_path 'lp/switch_up_btn_normal.png' %>');
  width: 58px;
  height: 58px;
  margin-top: 13px;
  margin-right: 71px;
  display: inline-block;
}

#switchbar:hover .arrow-up {
  background: url('<%= asset_path 'lp/switch_up_btn_mouseover.png' %>');
}

#switchbar:active .arrow-up {
  background: url('<%= asset_path 'lp/switch_up_btn_pressed.png' %>');
}

#switchbar-bottom {
